<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
	PUBLIC "-ibatis.apache.org//DTD SQL Map 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-2.dtd">
	
<sqlMap namespace="ad">

	<select id="getAd" parameterClass="String" resultClass="AdVO">
		select ad_img from ad where ad_code = #ad_code#
	</select>
	
	<select id="getAllAd" resultClass="AdVO">
		select * from AD	
	</select>
	
</sqlMap>